<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>
		
	<context-param>
	<param-name>movie</param-name>
	<param-value>Sholay</param-value>
	</context-param>
	
	<servlet>
		<servlet-name>myFirstServlet</servlet-name>
		<servlet-class>com.techchefs.mywebapp.servlets.MyFirstServlet</servlet-class>
		<init-param>
		<param-name>actor</param-name>
		<param-value>Gabbar</param-value>
		</init-param>
	</servlet>
	<servlet>
		<servlet-name>RedirectServlet</servlet-name>
		<display-name>RedirectServlet</display-name>
		<description></description>
		<servlet-class>com.techchefs.mywebapp.servlets.RedirectServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>ForwordServlet</servlet-name>
		<display-name>ForwordServlet</display-name>
		<description></description>
		<servlet-class>com.techchefs.mywebapp.servlets.ForwordServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>IncludeServlet</servlet-name>
		<display-name>IncludeServlet</display-name>
		<description></description>
		<servlet-class>com.techchefs.mywebapp.servlets.IncludeServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>myFirstServlet</servlet-name>
		<url-pattern>/currentDate</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>RedirectServlet</servlet-name>
		<url-pattern>/redirect</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ForwordServlet</servlet-name>
		<url-pattern>/forword</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>IncludeServlet</servlet-name>
		<url-pattern>/include</url-pattern>
	</servlet-mapping>
</web-app>
