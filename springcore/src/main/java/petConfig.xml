<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
	   xmlns:context="http://www.springframework.org/schema/context"
	   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	   xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
        http://www.springframework.org/schema/context
    	http://www.springframework.org/schema/context/spring-context-3.0.xsd">

	<context:component-scan base-package="com.techchefs.springcore.config"></context:component-scan>
	
	<bean id="petBean" class="com.techchefs.springcore.beans.PetBean">
		<property name="name" value="Pinku"></property>
	</bean>
	
    <bean id="animal" class="com.techchefs.springcore.exampleclass.Donkey">
    	
    </bean>
</beans>